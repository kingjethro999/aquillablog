{
    "version": 2,
    "builds": [
      {
        "src": "packages/frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "packages/frontend/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://aquilablog-api.onrender.com"
    }
  }